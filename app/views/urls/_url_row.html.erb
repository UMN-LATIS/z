<tr>
	<td><%= link_to url.id, url_path(url) %></td>
	<td><%= url.url %></td>
	<td><%= url.keyword %></td>
	<td><%= url.total_clicks %></td>
	<td><%= url.group_id %></td>
	<td><%= url.modified_by %></td>
	<td><%=l url.created_at %></td>
	<td>
		<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
								url_path(url),
								:method => :delete,
								:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
								:class => 'btn btn-xs btn-danger', :remote => true %>
	</td>
</tr>
