<%- model_class = Url -%>
<!-- <div class="page-header">
  <h4><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h3>
</div> -->

<div id="pending-transfer-requests-to">
  <%= render 'transfer_requests_to' %>
</div>

<div id="pending-transfer-requests-from">
  <%= render 'transfer_requests_from' %>
</div>

<div id="new-url-form">
  <%= render 'form', url: @url %>
</div>

<table class="table user-table table-striped table-bordered data-table">
  <thead>
    <tr>
      <th></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:keyword) %></th>
      <th><%= model_class.human_attribute_name(:total_clicks) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @urls.each do |url| %>
      <%= render 'in_row_show', url: url %>
    <% end %>
  </tbody>
</table>

<script type="text/javascript">
  <%= render 'datatable.js' %>
</script>

<%= render 'modal' %>
