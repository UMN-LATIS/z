
$(".stats-actions .js-transfer-urls").click(function(e){
  transferUrl("<%= new_transfer_request_path %>", "<%= @url.keyword %>");
})
function transferUrl(transferPath, keywords){
  $.ajax({
    url: transferPath,
    data: {keywords: keywords},
    dataType: 'script'
  });
}

