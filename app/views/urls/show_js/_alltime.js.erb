function drawChartAllTime() {
  if (google.visualization === undefined){
    return;
  }

  var data = new google.visualization.DataTable(<%= click_data_formatter(@clicks[:alltime]).to_json.html_safe %>);

  var options = {
    axes: {
      x: {
        0: {label: 'Month'}
      },
      y: {
        0: {label: "Clicks"}
      }
    },
    width: '100%',
    height: 255,
    legend: {
      position: 'none'
    }
  };

  var chart = new google.charts.Bar(document.getElementById('chart-div-all'));

  chart.draw(data, options);
}
