function drawChartDays30() {
  var data = new google.visualization.DataTable(<%= click_data_formatter(@clicks[:days30]).to_json.html_safe %>);

  var options = {
    axes: {
      x: {
        0: {label: 'Day'}
      },
      y: {
        0: {label: "Clicks"}
      }
    },
    width: '100%',
    height: 255,
    legend: {
      position: 'none'
    },
    colors: ['#BFAB25']
  };

  var chart = new google.charts.Bar(document.getElementById('chart-div-days30'));

  chart.draw(data, options);
}
