<tr id='user-<%= member.id %>'>
  <td><%= member.display_name %></td>
  <td><%= member.internet_id %></td>
  <td>
    <%= link_to t('.destroy', default: t('views.admin.members.index.table.destroy')),
                admin_member_path(member),
                :method => :delete,
                :data => {confirm: t('views.admin.members.index.table.destroy_confirm_html', name: member.display_name)},
                class: 'btn btn-xs btn-danger delete-admin-member fa fa-trash-o', remote: true %>

  </td>
</tr>
