datatable = $('table.data-table').DataTable()
datatable.draw();
row = $("tr#group-<%= @group.id %>")
datatable.rows( row ).remove();
datatable.rows.add($("<%= escape_javascript(render(partial: 'groups/in_row_show', locals: { group: @group })) %>")); // Add new data
datatable.columns.adjust().draw(); // Redraw the DataTable
