<tr id='result-<%= result.id %>'>
  <td><%= result.user_full_name %></td>
  <td><%= result.uid %></td>
  <td><%= result.admin %></td>
  <td><%= l result.created_at %></td>

  <td>
    <%= link_to t('.destroy', default: t("helpers.links.destroy")),
                group_member_path(@group, member),
                :method => :delete,
                :data => {confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?'))},
                class: 'btn btn-xs btn-danger delete-group-member', remote: true %>
  </td>
</tr>
